{
  "name": "employee-profile-management",
  "private": true,
  "version": "5.0.0",
  "type": "commonjs",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "client:dev": "vite --mode dev --port 3003",
    "server:dev": "tsx src/server/main.ts",
    "server:build": "babel --verbose --out-dir dist --only 'src/server/main.ts,src/server/' --extensions '.ts,.ts,.json' src",
    "client:build": "vite build",
    "build": "npm run client:build && npm run server:build",
    "serve": "vite preview",
    "test": "prettier --check . && eslint . && stylelint \"**/*.css\"",
    "start": "node dist/server/main.js"
  },
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@hookform/resolvers": "^3.1.1",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@mui/x-date-pickers": "^7.8.0",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "jddb": "^0.1.3",
    "lodash-es": "^4.17.21",
    "morgan": "^1.10.0",
    "notistack": "^3.0.1",
    "numeral": "^2.0.6",
    "path-to-regexp": "^6.2.1",
    "qs": "^6.11.2",
    "react": "latest",
    "react-dom": "latest",
    "react-fast-compare": "^3.2.2",
    "react-hook-form": "^7.45.1",
    "react-intersection-observer": "^9.10.3",
    "react-redux": "^8.1.1",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.14.1",
    "signal-exit": "^4.1.0",
    "tsx": "^4.16.0",
    "usehooks-ts": "^2.9.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-form-data": "^2.0.5",
    "@types/lodash-es": "^4.17.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.3.3",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "latest",
    "concurrently": "^8.2.2",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "2.6.2",
    "typescript": "^5.1.6",
    "vite": "latest"
  }
}
